{
    "name": "kasaikou",
    "dockerComposeFile": "./compose.yml",
    "service": "dev",
    "workspaceFolder": "/workspace",
    "postCreateCommand": "sh /workspace/github.com/kasaikou/kasaikou/.devcontainer/postCreateCommand.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "eamodio.gitlens",
                "mhutchie.git-graph",
                "chrisdias.vscode-opennewinstance"
            ],
            "settings": {
                "git.enableCommitSigning": true
            }
        }
    }
}